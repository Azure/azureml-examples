        },
        "local_path": {
          "title": "local_path",
          "type": "string",
          "description": "the path from which the artifact gets uploaded to the cloud"
        },
        "name": {
          "title": "name",
          "type": "string"
        },
        "$schema": {
          "title": "$schema",
          "type": "string",
          "readonly": true
        },
        "tags": {
          "title": "tags",
          "type": "object",
          "additionalProperties": {}
        },
        "version": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "version"
        }
      },
      "additionalProperties": false
    },
    "CreationContextSchema": {
      "type": "object",
      "properties": {
        "created_at": {
          "title": "created_at",
          "type": "string",
