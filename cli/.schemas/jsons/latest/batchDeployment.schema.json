        "port": {
          "title": "port",
          "type": "number",
          "format": "integer"
        }
      },
      "required": [
        "path",
        "port"
      ],
      "additionalProperties": false
    },
    "AnonymousEnvironmentSchema": {
      "type": "object",
      "properties": {
        "build": {
          "type": "object",
