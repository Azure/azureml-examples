    "AnonymousModelSchema": {
      "type": "object",
      "properties": {
        "creation_context": {
          "type": "object",
          "$ref": "#/definitions/CreationContextSchema"
        },
        "datastore": {
          "type": "string",
          "pattern": "^azureml:.*",
          "arm_type": "datastores",
          "title": "datastore"
        },
        "description": {
          "title": "description",
          "type": "string"
        },
        "flavors": {
          "title": "flavors",
          "type": "object",
          "additionalProperties": {}
        },
        "id": {
          "type": "string",
          "pattern": "^azureml:.*",
          "arm_type": "models",
          "title": "id",
          "readonly": true
        },
        "job_name": {
          "title": "job_name",
          "type": "string",
          "readonly": true
        },
        "local_path": {
          "title": "local_path",
          "type": "string"
        },
        "model_format": {
          "type": "string",
          "enum": [
            "custom",
            "mlflow",
            "triton",
            "openai"
          ],
          "title": "model_format"
        },
        "model_uri": {
          "title": "model_uri",
          "type": "string"
        },
        "name": {
          "title": "name",
          "type": "string"
        },
        "properties": {
          "title": "properties",
