        "readiness_route": {
          "type": "object",
          "$ref": "#/definitions/RouteSchema"
        },
        "scoring_route": {
          "type": "object",
          "$ref": "#/definitions/RouteSchema"
        }
      },
      "required": [
        "liveness_route",
        "readiness_route",
        "scoring_route"
