            },
            {
              "type": "object",
              "$ref": "#/definitions/CodeAssetSchema"
            },
            {
              "type": "string",
              "pattern": "^file:.*"
            }
          ]
        },
        "$schema": {
          "title": "$schema",
          "type": "string",
          "readonly": true
        },
        "scoring_script": {
          "title": "scoring_script",
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "CodeAssetSchema": {
